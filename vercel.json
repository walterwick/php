{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "rewrites": [
    { "source": "/api/(.*\\.php)$", "destination": "/api/$1" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/m/(.*)", "destination": "/m/$1" },
    {
      "source": "/m/:path*\\.html",
      "destination": "/m/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
